  "license": "GPL-3.0",
  "private": true,
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "start": "vercel dev",
    "predev": "kill-port 8787 && docker compose up -d && prisma format && prisma generate",
    "dev": "dotenvx run -- wrangler dev api/index.ts",
    "deploy:vercel": "vercel",
    "lint": "eslint \"api/**/*.ts\" --fix",
    "format": "prisma format && prettier --cache --write .",
    "pretest": "prisma format && prisma generate",
    "test": "jest || true"
  },
  "dependencies": {
    "@prisma/client": "6.5.0",
    "hono": "4.7.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250319.0",
    "@dotenvx/dotenvx": "1.39.0",
    "@eslint/js": "9.22.0",
    "@prisma/extension-accelerate": "1.3.0",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.11",
    "eslint": "9.22.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "prettier": "3.5.3",
    "prisma": "6.5.0",
    "ts-jest": "29.2.6",
    "ts-node": "10.9.2",
    "typescript": "5.8.2",
    "typescript-eslint": "8.27.0",
    "vercel": "41.4.1",
    "wrangler": "4.3.0"
  },
  "keywords": [
    "hono"
  ]
