{
  "name": "@ts/rest-hono",
  "version": "0.0.1",
  "description": "TypeScript - Hono - Rest",
  "main": "index.js",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
  "license": "GPL-3.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "start": "vercel dev",
    "predev": "kill-port 8787 && docker compose up -d && prisma format && prisma generate",
    "dev": "dotenvx run -- wrangler dev api/index.ts",
    "deploy:vercel": "vercel",
    "lint": "eslint \"api/**/*.ts\" --fix",
    "format": "prisma format && prettier --cache --write .",
    "pretest": "prisma format && prisma generate",
    "test": "jest || true"
  },
  "dependencies": {
    "@prisma/client": "6.2.1",
    "hono": "4.6.16"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250109.0",
    "@dotenvx/dotenvx": "1.32.1",
    "@eslint/js": "9.18.0",
    "@prisma/extension-accelerate": "1.2.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.6",
    "eslint": "9.18.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "prettier": "3.4.2",
    "prisma": "6.2.1",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "typescript-eslint": "8.19.1",
    "vercel": "39.2.6",
    "wrangler": "3.101.0"
  },
  "keywords": [
    "hono"
  ]
}
