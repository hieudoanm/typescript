{
  "name": "@ts/graphql-apollo",
  "version": "0.0.1",
  "description": "TypeScript - GraphQL - Apollo",
  "main": "dist/index",
  "repository": "git@github.com:hieudoanm/hieudoanm.git",
  "author": "hieudoanm <hieumdoan@gmail.com> (https://hieudoanm.github.io)",
  "license": "GPL-3.0",
  "private": true,
  "packageManager": "pnpm@9.15.3",
  "scripts": {
    "format": "prettier --cache --write .",
    "serve": "webpack serve --mode=development",
    "dev": "concurrently \"pnpm codegen\" \"pnpm webpack:watch\" \"pnpm dev:nodemon\"",
    "dev:nodemon": "nodemon ./dist/main.js --watch dist",
    "webpack:watch": "webpack --progress --watch",
    "start": "kill-port 5000 && node ./dist/main",
    "prebuild": "pnpm codegen",
    "build": "webpack --mode=production",
    "test": "jest",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "codegen": "graphql-codegen --config codegen.ts || true"
  },
  "dependencies": {
    "@apollo/datasource-rest": "6.4.1",
    "@apollo/server": "4.11.3",
    "@graphql-tools/schema": "10.0.16",
    "graphql": "16.10.0",
    "graphql-tag": "2.12.6",
    "pino": "9.6.0"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@graphql-codegen/cli": "5.0.3",
    "@graphql-codegen/introspection": "4.0.3",
    "@graphql-codegen/typescript": "4.1.2",
    "@graphql-codegen/typescript-resolvers": "4.4.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.10.5",
    "@webpack-cli/generators": "3.0.7",
    "concurrently": "9.1.2",
    "eslint": "9.17.0",
    "globals": "15.14.0",
    "jest": "29.7.0",
    "kill-port": "2.0.1",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "ts-jest": "29.2.5",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typescript": "5.7.2",
    "typescript-eslint": "8.19.1",
    "webpack": "5.97.1",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.0"
  }
}
